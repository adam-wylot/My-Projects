@model List<Towar>

<table class="table table-stripped">
    <tr>
        <th>Nazwa</th>
        <th>Typ</th>
        <th></th>
    </tr>
    @foreach(var item in Model)
    {
        <tr>
            <td>@item.Nazwa</td>
            <td>@item.Typ</td>
            <td><a asp-controller="Order" asp-action="RemoveFromBasket" asp-route-id="@item.Id" class="btn btn-primary">Usuń</a></td>
        </tr>
    }
</table>
